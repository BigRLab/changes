{
  "linters": {
    "json": {
      "type": "json",
      "include": [
        "(^\\.arcconfig$)",
        "(^\\.arclint$)",
        "(\\.json$)"
      ]
    },
    "spelling": {
      "type": "spelling"
    },
    "python": {
      "type": "flake8",
      "include": [
        "(tests/*)",
        "(changes/*)"
      ]
    }
  }
}
